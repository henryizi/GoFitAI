{
  "expo": {
    "name": "GoFitAI",
    "slug": "gofitai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-converted.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-converted.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.henrymadeit.gofitai",
      "buildNumber": "6",
      "infoPlist": {
        "LSMinimumSystemVersion": "13.4",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "gofitai-production.up.railway.app": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSIncludesSubdomains": true
            },
            "up.railway.app": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSIncludesSubdomains": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-converted.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.henrymadeit.gofitai",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon-128.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GoFitAI to access your camera to take photos of your food and progress."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GoFitAI to access your photos to upload food and progress images."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#FF6B35",
          "sounds": ["./assets/notification.wav"]
        }
      ],
      "sentry-expo"
    ],
    "scheme": "gofitai",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8acc1562-09e6-45d8-a2ec-5689e73600e2"
      },
      "EXPO_PUBLIC_API_URL": "https://gofitai-production.up.railway.app",
