{
  "expo": {
    "name": "GoFitAI",
    "slug": "gofitai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-converted.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-converted.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.henrymadeit.gofitai",
      "buildNumber": "1",
      "deploymentTarget": "16.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-converted.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.henrymadeit.gofitai",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon-128.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GoFitAI to access your camera to take photos of your food and progress."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GoFitAI to access your photos to upload food and progress images."
        }
      ],
      "sentry-expo"
    ],
    "scheme": "gofitai",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8acc1562-09e6-45d8-a2ec-5689e73600e2"
      },
      "EXPO_PUBLIC_API_URL": "https://gofitai-production.up.railway.app",
      "EXPO_PUBLIC_AI_TIMEOUT_MS": "45000",
      "EXPO_PUBLIC_AI_VERBOSE": "0"
    },
    "owner": "henryizi"
  }
}
