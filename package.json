{
  "name": "gofitai",
  "version": "1.0.0",
  "description": "AI-powered fitness and nutrition app",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "debug-revenuecat": "node -e \"require('dotenv').config(); console.log('RevenueCat API Key:', process.env.EXPO_PUBLIC_REVENUECAT_IOS_API_KEY ? 'Set ✅' : 'Missing ❌');\"",
    "test": "node test-recipe-generation.js && node test-food-analysis.js",
    "build": "echo 'No build step required for server'",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "ai",
    "fitness",
    "nutrition",
    "workout",
    "health",
    "bodybuilding"
  ],
  "author": "GoFitAI Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@google/generative-ai": "^0.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "expo": "^53.0.22",
    "expo-application": "~6.1.5",
    "expo-blur": "^14.1.5",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.6",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "^2.2.3",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "moti": "^0.30.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "posthog-react-native": "^4.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
