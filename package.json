{
  "name": "gofitai",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "kill-server": "lsof -ti:4000 | xargs kill -9 || true",
    "server": "cd server && node index.js",
    "start-server": "cd server && node start-server.js",
    "start-server-safe": "npm run kill-server && cd server && node start-server.js",
    "dev": "concurrently \"npm run start-server-safe\" \"expo start --clear\"",
    "dev-android": "concurrently \"npm run start-server-safe\" \"expo start --android --clear\"",
    "dev-ios": "concurrently \"npm run start-server-safe\" \"expo start --ios --clear\"",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "config": "expo config --type public | cat",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx | cat",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix | cat",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
