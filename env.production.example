# üöÄ PRODUCTION ENVIRONMENT CONFIGURATION
# Copy this file to .env.production for production builds

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_production_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key_here

# AI Service Configuration (DeepSeek handled by server)
EXPO_PUBLIC_AI_TIMEOUT_MS=45000
EXPO_PUBLIC_AI_VERBOSE=0

# PRODUCTION API Server (Railway)
EXPO_PUBLIC_API_URL=https://gofitai-production.up.railway.app

# Storage Configuration
EXPO_PUBLIC_SUPABASE_STORAGE_URL=your_production_supabase_storage_url_here

# Monitoring / Analytics
EXPO_PUBLIC_SENTRY_DSN=your_production_sentry_dsn_here
EXPO_PUBLIC_POSTHOG_KEY=your_production_posthog_project_api_key_here
EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# App Configuration
EXPO_PUBLIC_APP_NAME=GoFitAI
EXPO_PUBLIC_APP_VERSION=1.0.0

# ‚ö†Ô∏è IMPORTANT: Remove or comment out local development URLs
# EXPO_PUBLIC_API_URL=http://192.168.0.199:4000
# NGROK_AUTHTOKEN=your_ngrok_authtoken_here

# Server-only flags (in server .env)
# When true, server will not use rule-based fallback for nutrition analysis
AI_STRICT_ONLY=true
